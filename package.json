{
  "name": "laravel-debug-buddy",
  "displayName": "Laravel Debug Buddy",
  "description": "A powerful helper for debugging Laravel code with instant logging and dumping.",
  "version": "2.0.4",
  "publisher": "KeyurSanghani",
  "icon": "images/logo.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/Keyur2003/laravel-log-helper.git"
  },
  "engines": {
    "vscode": "^1.96.0"
  },
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "laravel-debug-buddy.logVariable",
        "title": "Log Variable"
      },
      {
        "command": "laravel-debug-buddy.ddVariable",
        "title": "DD Variable"
    }
    ],
    "keybindings": [
      {
        "command": "laravel-debug-buddy.logVariable",
        "key": "ctrl+shift+l",
        "mac": "cmd+shift+l",
        "when": "editorTextFocus"
      },
      {
        "command": "laravel-debug-buddy.ddVariable",
        "key": "ctrl+shift+d",
        "mac": "cmd+shift+d",
        "when": "editorTextFocus"
    }
    ]
  }
}
