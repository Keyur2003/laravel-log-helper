{
  "name": "laravel-debug-buddy",
  "displayName": "Laravel Debug Buddy",
  "description": "A powerful helper for debugging Laravel code with instant logging, dumping, and execution timing.",
  "version": "3.0.2",
  "publisher": "KeyurSanghani",
  "icon": "images/logo.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/Keyur2003/laravel-log-helper.git"
  },
  "engines": {
    "vscode": "^1.96.0"
  },
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "laravel-debug-buddy.logVariable",
        "title": "Log Variable"
      },
      {
        "command": "laravel-debug-buddy.ddVariable",
        "title": "DD Variable"
      },
      {
        "command": "laravel-debug-buddy.removeDebugStatements",
        "title": "Remove Debug Statements"
      },
      {
        "command": "laravel-debug-buddy.timeExecution",
        "title": "Time Execution"
      }
    ],
    "keybindings": [
      {
        "command": "laravel-debug-buddy.logVariable",
        "key": "ctrl+shift+l",
        "mac": "cmd+shift+l",
        "when": "editorTextFocus"
      },
      {
        "command": "laravel-debug-buddy.ddVariable",
        "key": "ctrl+shift+d",
        "mac": "cmd+shift+d",
        "when": "editorTextFocus"
      },
      {
        "command": "laravel-debug-buddy.removeDebugStatements",
        "key": "ctrl+shift+r",
        "mac": "cmd+shift+r",
        "when": "editorTextFocus"
      },
      {
        "command": "laravel-debug-buddy.timeExecution",
        "key": "ctrl+shift+t",
        "mac": "cmd+shift+t",
        "when": "editorTextFocus"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "laravel-debug-buddy.logVariable",
          "group": "laravel-debug-buddy"
        },
        {
          "command": "laravel-debug-buddy.ddVariable",
          "group": "laravel-debug-buddy"
        },
        {
          "command": "laravel-debug-buddy.removeDebugStatements",
          "group": "laravel-debug-buddy"
        },
        {
          "command": "laravel-debug-buddy.timeExecution",
          "group": "laravel-debug-buddy"
        }
      ]
    }
  },
  "activationEvents": [
    "onCommand:laravel-debug-buddy.logVariable",
    "onCommand:laravel-debug-buddy.ddVariable",
    "onCommand:laravel-debug-buddy.removeDebugStatements",
    "onCommand:laravel-debug-buddy.timeExecution"
  ],
  "categories": [
    "Debuggers",
    "Other"
  ],
  "keywords": [
    "laravel",
    "debug",
    "logging",
    "dd",
    "php",
    "productivity"
  ],
  "bugs": {
    "url": "https://github.com/Keyur2003/laravel-log-helper/issues"
  },
  "homepage": "https://github.com/Keyur2003/laravel-log-helper#readme",
  "license": "MIT"
}