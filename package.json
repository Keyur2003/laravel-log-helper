{
  "name": "laravel-debug-buddy",
  "displayName": "Laravel Debug Buddy",
  "description": "A powerful helper for debugging Laravel code with instant logging, dumping, and execution timing.",
  "version": "6.0.0",
  "publisher": "KeyurSanghani",
  "icon": "images/logo.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/Keyur2003/laravel-log-helper.git"
  },
  "engines": {
    "vscode": "^1.96.0"
  },
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "laravel-debug-buddy.logVariable",
        "title": "Log Variable"
      },
      {
        "command": "laravel-debug-buddy.ddVariable",
        "title": "DD Variable"
      },
      {
        "command": "laravel-debug-buddy.removeDebugStatements",
        "title": "Remove Debug Statements"
      },
      {
        "command": "laravel-debug-buddy.timeExecution",
        "title": "Time Execution"
      },
      {
        "command": "laravel-debug-buddy.quickLogVariable",
        "title": "Quick Log Selected Code"
      },
      {
        "command": "laravel-debug-buddy.quickDdVariable",
        "title": "Quick DD Selected Code"
      },
      {
        "command": "laravel-debug-buddy.queryLogger",
        "title": "Add Query Logger"
      },
      {
        "command": "laravel-debug-buddy.exceptionCatcher",
        "title": "Wrap with Exception Catcher"
      },
      {
        "command": "laravel-debug-buddy.requestResponseLogger",
        "title": "Add Request/Response Logger"
      },
      {
        "command": "laravel-debug-buddy.conditionalDebugger",
        "title": "Add Conditional Debugging"
      },
      {
        "command": "laravel-debug-buddy.methodCallTracer",
        "title": "Add Method Call Tracer"
      }
    ],
    "keybindings": [
      {
        "command": "laravel-debug-buddy.logVariable",
        "key": "ctrl+shift+l",
        "mac": "cmd+shift+l",
        "when": "editorTextFocus"
      },
      {
        "command": "laravel-debug-buddy.ddVariable",
        "key": "ctrl+shift+d",
        "mac": "cmd+shift+d",
        "when": "editorTextFocus"
      },
      {
        "command": "laravel-debug-buddy.removeDebugStatements",
        "key": "ctrl+shift+r",
        "mac": "cmd+shift+r",
        "when": "editorTextFocus"
      },
      {
        "command": "laravel-debug-buddy.timeExecution",
        "key": "ctrl+shift+t",
        "mac": "cmd+shift+t",
        "when": "editorTextFocus"
      },
      {
        "command": "laravel-debug-buddy.quickLogVariable",
        "key": "ctrl+shift+q",
        "mac": "cmd+shift+q",
        "when": "editorTextFocus"
      },
      {
        "command": "laravel-debug-buddy.quickDdVariable",
        "key": "ctrl+shift+w",
        "mac": "cmd+shift+w",
        "when": "editorTextFocus"
      },
      {
        "command": "laravel-debug-buddy.queryLogger",
        "key": "ctrl+shift+1",
        "mac": "cmd+shift+1",
        "when": "editorTextFocus"
      },
      {
        "command": "laravel-debug-buddy.exceptionCatcher",
        "key": "ctrl+shift+2",
        "mac": "cmd+shift+2",
        "when": "editorTextFocus"
      },
      {
        "command": "laravel-debug-buddy.requestResponseLogger",
        "key": "ctrl+shift+3",
        "mac": "cmd+shift+3",
        "when": "editorTextFocus"
      },
      {
        "command": "laravel-debug-buddy.conditionalDebugger",
        "key": "ctrl+shift+4",
        "mac": "cmd+shift+4",
        "when": "editorTextFocus"
      },
      {
        "command": "laravel-debug-buddy.methodCallTracer",
        "key": "ctrl+shift+5",
        "mac": "cmd+shift+5",
        "when": "editorTextFocus"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "laravel-debug-buddy.logVariable",
          "group": "laravel-debug-buddy"
        },
        {
          "command": "laravel-debug-buddy.ddVariable",
          "group": "laravel-debug-buddy"
        },
        {
          "command": "laravel-debug-buddy.removeDebugStatements",
          "group": "laravel-debug-buddy"
        },
        {
          "command": "laravel-debug-buddy.timeExecution",
          "group": "laravel-debug-buddy"
        },
        {
          "command": "laravel-debug-buddy.quickLogVariable",
          "group": "laravel-debug-buddy"
        },
        {
          "command": "laravel-debug-buddy.quickDdVariable",
          "group": "laravel-debug-buddy"
        },
        {
          "command": "laravel-debug-buddy.queryLogger",
          "group": "laravel-debug-buddy"
        },
        {
          "command": "laravel-debug-buddy.exceptionCatcher",
          "group": "laravel-debug-buddy"
        },
        {
          "command": "laravel-debug-buddy.requestResponseLogger",
          "group": "laravel-debug-buddy"
        },
        {
          "command": "laravel-debug-buddy.conditionalDebugger",
          "group": "laravel-debug-buddy"
        },
        {
          "command": "laravel-debug-buddy.methodCallTracer",
          "group": "laravel-debug-buddy"
        }
      ]
    }
  },
  "activationEvents": [
    "onCommand:laravel-debug-buddy.logVariable",
    "onCommand:laravel-debug-buddy.ddVariable",
    "onCommand:laravel-debug-buddy.removeDebugStatements",
    "onCommand:laravel-debug-buddy.timeExecution",
    "onCommand:laravel-debug-buddy.quickLogVariable",
    "onCommand:laravel-debug-buddy.quickDdVariable",
    "onCommand:laravel-debug-buddy.queryLogger",
    "onCommand:laravel-debug-buddy.exceptionCatcher",
    "onCommand:laravel-debug-buddy.requestResponseLogger",
    "onCommand:laravel-debug-buddy.conditionalDebugger",
    "onCommand:laravel-debug-buddy.methodCallTracer"
  ],
  "categories": [
    "Debuggers",
    "Other"
  ],
  "keywords": [
    "laravel",
    "debug",
    "logging",
    "dd",
    "php",
    "productivity"
  ],
  "bugs": {
    "url": "https://github.com/Keyur2003/laravel-log-helper/issues"
  },
  "homepage": "https://github.com/Keyur2003/laravel-log-helper#readme",
  "license": "MIT"
}
