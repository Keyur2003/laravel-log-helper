{
  "name": "laravel-log-helper",
  "displayName": "Laravel Log Helper",
  "description": "A helper to log variables in Laravel.",
  "version": "2.0.1",
  "publisher": "KeyurSanghani",
  "repository": {
    "type": "git",
    "url": "https://github.com/Keyur2003/laravel-log-helper.git"
  },
  "engines": {
    "vscode": "^1.96.0"
  },
  "main": "./extension.js",
  "activationEvents": [
    "onCommand:laravel-log-helper.logVariable"
  ],
  "contributes": {
    "commands": [
      {
        "command": "laravel-log-helper.logVariable",
        "title": "Log Variable"
      },
      {
        "command": "laravel-log-helper.ddVariable",
        "title": "DD Variable"
    }
    ],
    "keybindings": [
      {
        "command": "laravel-log-helper.logVariable",
        "key": "ctrl+shift+l",
        "mac": "cmd+shift+l",
        "when": "editorTextFocus"
      },
      {
        "command": "laravel-log-helper.ddVariable",
        "key": "ctrl+shift+d",
        "mac": "cmd+shift+d",
        "when": "editorTextFocus"
    }
    ]
  }
}
